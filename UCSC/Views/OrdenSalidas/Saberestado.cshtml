
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
    <link href="~/Content/BootstrapLogin.css" rel="stylesheet" id="bootstrap-css">
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>



</head>
<body>
    <div>

        <div class="wrapper fadeInDown">
            <div id="formContent">
   
                <div class="fadeIn first">
                    <img src="https://i.ibb.co/HnMSrgQ/descarga.jpg" id="icon" alt="User Icon" />
                </div>
                <!-- Login Form -->
                <form action="@Url.Action("Saberestado")" method="post">
                    <input type="text" id="login" class="fadeIn second" name="estadoreal" placeholder="Ingresa numero de Orden">
                    <input type="submit" class="fadeIn fourth" value="Buscar">
                </form>

            </div>
        </div>

    </div>
</body>
</html>

<div class="modal fade" id="modalEdit"></div>

<script>
    $(document).ready(function () {
        $('#submit').on('click', function (e) {
            e.preventDefault();
            $('#modalEdit').modal('show');
        });

    });

</script>